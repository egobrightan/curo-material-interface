  .grid-row
    .grid-col-12
      %h2.cmi-h2.cmi-lined
        %span.cmi-span Loading indicator
      .grid-row
        .grid-col-6
          #loading-indicator-1{ style: 'width: 100%; height: 150px; background: #f9f9f9; position: relative;' }
            .cmi-loading-indicator.cmi-loading-fade
              .cmi-loading-indicator-inside
                %svg.cmi-circular
                  %circle.cmi-path{ cx: '50', cy: '50', r: '20', fill: 'none', :'stroke-width' => '3', :'stroke-miterlimit' => '10' }
        .grid-col-6
          %h3.cmi-h3 Javascript
      .grid-row
        .grid-col-6
          %h3 Loading default
          #loading-indicator-2{ style: 'width: 100%; height: 150px; background: #f9f9f9; position: relative;' }
        .grid-col-6
          %br/
          %br/
          %pre.cmi-pre
            :preserve
              var loading = new CMI.LoadingIndicator();
              $('#loading-indicator-2').append(loading.domElement());
              loading.fadeIn();
              // loading.fadeOut()
              // loading.remove()

        :javascript
          $(function() {
            var loading = new CMI.LoadingIndicator();
            $('#loading-indicator-2').append(loading.domElement());
            loading.fadeIn();
          });

