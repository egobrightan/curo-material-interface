    .grid-col-6
      %pre.cmi-pre
        :preserve
          $('#tabs-1 li.cmi-tabs-active a').each(function() {
            CMI.Tabs.activate($(this));
          });
          $('#tabs-1').on('click', 'a', function(event) {
            event.preventDefault();
            CMI.Tabs.activate($(this));
